    "d3": "^3.5.17",
    "debug": "^3.2.7",
    "dompurify": "^2.0.17",
    "elliptic": "^6.6.0",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.3.0",
    "markdown": "0.5.0",
    "material-design-iconic-font": "^2.2.0",
    "mousetrap": "^1.6.1",
    "mustache": "^2.3.0",
    "numeral": "^2.0.6",
