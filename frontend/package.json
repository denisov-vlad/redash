    "@babel/plugin-transform-react-jsx-self": "^7.25.9",
    "@babel/plugin-transform-react-jsx-source": "^7.25.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@cypress/code-coverage": "^3.11.0",
