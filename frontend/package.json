    "raw-loader": "^0.5.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^17.0.0",
    "request-cookies": "^1.1.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-build-notifier": "^2.3.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest": {
    "rootDir": "./client",
    "setupFiles": [
      "./app/__tests__/enzyme_setup.js",
      "./app/__tests__/mocks.js"
    ],
    "setupFilesAfterEnv": [
      "../jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/app/$1",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/app/__tests__/"
    ]
  },
  "nyc": {
    "include": [
      "client/app/**",
      "viz-lib/**"
    ]
  },
  "browser": {
    "fs": false,
    "path": false
  },
  "browserslist": [
    "Edge >= 15",
    "Firefox >= 52",
    "Chrome >= 55",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Opera >= 42",
    "op_mob >= 46",
    "android >= 67",
    "and_chr >= 71",
    "and_ff >= 64",
    "and_uc >= 11.8",
    "samsung >= 6.2"
  ],
  "packageManager": "yarn@4.9.1"
}
